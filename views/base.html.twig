    <!-- Style css -->
    <link rel="stylesheet" href="/assets/css/style.css">
    {% endblock %}
</head>
<body>
    {% block header %}
    {% include "layout/_nav.html.twig" %}
    {% endblock %}

    {% block body %}{% endblock %}
    <div class="mouse-cursor cursor-outer"></div>
